pipeline {
    agent any
    tools {
        maven 'M2_HOME'
    }
    stages{stage('sonarqubescan'){
            steps{
                withSonarQubeEnv('sonarserver'){ 
                
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=kinjnr77_week16-code'
           }
        }
      }

        stage("all maven commands"){
            steps{
                sh 'mvn clean test compile install package'
            }
        }
    }
}